<%- include("../../views/partials/user/header.ejs") %>
    <%- include("../../views/partials/user/navBar") %>


        <link rel="stylesheet" href="/css/profile.css">


        <div class="profile-session-container">
            <div class="container-fluid">
                <div class="dashboard">
                    <!-- Sidebar -->
                    <%- include("../../views/partials/user/profileSideBar") %>

                        <!-- Main Content -->
                        <div class="main-content">

                            <div class="dash">
                                <!-- Header Section -->
                                <div class="head1 text-center mb-4">
                                    <h2 class="dashboard-title text-success fw-bold">Dashboard</h2>
                                </div>

                                <!-- Profile Overview Section -->
                                <div class="profile-overview mb-5">
                                    <h3 class="section-title text-secondary border-bottom pb-2">
                                        <%= userData.name   %>
                                    </h3>
                                    <div class="profile-info mt-4">
                                        <p class="mb-3">
                                            <strong>Name:</strong>
                                            <span class="text-dark">
                                                <%= userData.name   %>
                                            </span>
                                        </p>
                                        <p class="mb-3">
                                            <strong>Email:</strong>
                                            <span class="text-dark">
                                                <%= userData.email   %>
                                            </span>
                                        </p>
                                        <p class="mb-3">
                                            <strong>Phone:</strong>
                                            <span class="text-muted">
                                                
                                                <%= userData.phone || "null"  %>
                                            </span>
                                        </p>
                                    </div>
                                </div>
                            </div>


                        </div>
                </div>
            </div>
        </div>